# Alpha Application
APP_NAME=OctavaDigital-Alpha
APP_ENV=alpha
APP_KEY=base64:alpha123456789alpha123456789alpha12
APP_DEBUG=true
APP_URL=https://alpha.octavadigital.cl
APP_TIMEZONE=America/Santiago
APP_LOCALE=es
APP_CURRENCY=CLP

# Alpha Database
DB_CONNECTION=mysql
DB_HOST=alpha-db.octavadigital.cl
DB_PORT=3306
DB_DATABASE=alpha_octavadigital
DB_USERNAME=alpha_user
DB_PASSWORD=alpha_password
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci

# Alpha Redis
REDIS_HOST=alpha-redis.octavadigital.cl
REDIS_PASSWORD=alpha_redis_password
REDIS_PORT=6379
REDIS_DB=0

# Alpha Mail (Development)
MAIL_MAILER=log
MAIL_FROM_ADDRESS=alpha@octavadigital.cl
MAIL_FROM_NAME="${APP_NAME} Alpha"

# Alpha Storage
AWS_ACCESS_KEY_ID=alpha_aws_key
AWS_SECRET_ACCESS_KEY=alpha_aws_secret
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=alpha-octavadigital
AWS_URL=https://alpha-cdn.octavadigital.cl
AWS_ENDPOINT=https://s3.us-east-1.amazonaws.com
AWS_USE_PATH_STYLE_ENDPOINT=false

# Alpha Program Settings
ALPHA_ENABLED=true
ALPHA_VERSION=0.1.0
ALPHA_BUILD=${CI_BUILD_NUMBER}
ALPHA_COMMIT=${CI_COMMIT_SHA}
ALPHA_BRANCH=${CI_BRANCH}
ALPHA_ACCESS_CODE=ALPHA2024

# Alpha Testing
ALPHA_TEST_MODE=true
ALPHA_DEBUG_MODE=true
ALPHA_STRICT_MODE=false
ALPHA_EXPERIMENTAL=true
ALPHA_UNSTABLE=true

# Alpha Features
ENABLE_EXPERIMENTAL=true
ENABLE_UNSTABLE=true
ENABLE_DEBUG_TOOLS=true
ENABLE_TESTING_TOOLS=true
ALPHA_FEATURE_FLAGS="all"

# Alpha Access
ALPHA_ACCESS_RESTRICTED=true
ALPHA_INTERNAL_ONLY=true
ALPHA_TEAM_ONLY=true
ALPHA_MAX_USERS=50
ALPHA_EXPIRES_AT="+30 days"

# Alpha Development
DEBUG_LEVEL=verbose
LOG_LEVEL=debug
QUERY_LOG=true
EVENT_LOG=true
CACHE_LOG=true
ERROR_REPORTING=-1
DISPLAY_ERRORS=true

# Alpha Testing Tools
ALPHA_TESTING_ENABLED=true
ALPHA_PROFILER_ENABLED=true
ALPHA_DEBUGBAR_ENABLED=true
ALPHA_TELESCOPE_ENABLED=true
ALPHA_QUERY_DETECTOR=true

# Alpha Monitoring
SENTRY_LARAVEL_DSN=https://alpha@sentry.octavadigital.cl/123
SENTRY_TRACES_SAMPLE_RATE=1.0
NEW_RELIC_LICENSE_KEY=alpha_newrelic_key
NEW_RELIC_APP_NAME="Octava Digital Alpha"

# Alpha Feedback
ALPHA_FEEDBACK_ENABLED=true
ALPHA_FEEDBACK_EMAIL=alpha.feedback@octavadigital.cl
ALPHA_FEEDBACK_SLACK=https://hooks.slack.com/services/alpha/channel
ALPHA_FEEDBACK_JIRA=https://jira.octavadigital.cl/alpha

# Alpha Notifications
ALPHA_NOTIFICATIONS_ENABLED=true
ALPHA_NOTIFICATION_EMAIL=alpha.notifications@octavadigital.cl
ALPHA_NOTIFICATION_SLACK=true
ALPHA_NOTIFICATION_DISCORD=true

# Alpha Security (Development)
SESSION_DRIVER=file
SESSION_LIFETIME=240
SESSION_SECURE_COOKIE=false
SANCTUM_STATEFUL_DOMAINS=alpha.octavadigital.cl,localhost

# Alpha Payment Gateways (Test)
STRIPE_KEY=pk_test_alpha
STRIPE_SECRET=sk_test_alpha
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=alpha_paypal_id
PAYPAL_CLIENT_SECRET=alpha_paypal_secret

# Alpha Paths
STORAGE_PATH=/var/www/alpha/storage
PUBLIC_PATH=/var/www/alpha/public
UPLOAD_PATH=/var/www/alpha/uploads
LOG_PATH=/var/www/alpha/logs

# Alpha Notice
ALPHA_NOTICE=true
ALPHA_NOTICE_TEXT="⚠️ Alpha Testing - Unstable Features"
ALPHA_BANNER_TEXT="Alpha Version ${ALPHA_VERSION} - Internal Testing Only"
ALPHA_WATERMARK=true

# Alpha Development Tools
ALPHA_DEV_TOOLS=true
ALPHA_API_DOCS=true
ALPHA_SWAGGER_UI=true
ALPHA_GRAPHQL_PLAYGROUND=true

# Alpha Testing Features
ALPHA_FEATURE_TOGGLES=true
ALPHA_A_B_TESTING=true
ALPHA_PERFORMANCE_TESTING=true
ALPHA_LOAD_TESTING=true

# Alpha Metrics
ALPHA_METRICS_ENABLED=true
ALPHA_METRICS_INTERVAL=60
ALPHA_METRICS_DETAILED=true
ALPHA_PERFORMANCE_METRICS=true

# Alpha Debugging
ALPHA_DEBUG_TOOLBAR=true
ALPHA_DEBUG_QUERIES=true
ALPHA_DEBUG_EVENTS=true
ALPHA_DEBUG_CACHE=true
ALPHA_DEBUG_MAIL=true

# Alpha Error Handling
ALPHA_ERROR_DETAIL=true
ALPHA_ERROR_TRACE=true
ALPHA_ERROR_CONTEXT=true
ALPHA_ERROR_REPORTING=true

# Alpha Development Settings
ALPHA_HOT_RELOAD=true
ALPHA_AUTO_REFRESH=true
ALPHA_LIVE_RELOAD=true
ALPHA_SOURCE_MAPS=true

# Alpha API Settings
API_DEBUG=true
API_STRICT=false
API_THROTTLE=0,1
API_VERSION=alpha-v1

# Alpha Cache Settings
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
BROADCAST_DRIVER=log
FILESYSTEM_DISK=local

# Alpha Team Access
ALPHA_TEAM_EMAILS="dev@octavadigital.cl,test@octavadigital.cl"
