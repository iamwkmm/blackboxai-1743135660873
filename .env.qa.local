# QA Local Overrides
# Personal QA testing configuration - DO NOT COMMIT

# Application
APP_NAME=OctavaDigital-QALocal
APP_ENV=qa
APP_KEY=base64:qa123456789qa123456789qa1234local
APP_DEBUG=true
APP_URL=https://qa.local.octavadigital.cl

# QA Database
DB_HOST=qa-local-db.octavadigital.cl
DB_PORT=3306
DB_DATABASE=qa_local_octavadigital
DB_USERNAME=qa_local_user
DB_PASSWORD=qa_local_password_123

# QA Redis
REDIS_HOST=qa-local-redis.octavadigital.cl
REDIS_PASSWORD=qa_local_redis_pass
REDIS_PORT=6379

# Personal Testing Paths
STORAGE_PATH=/var/www/qa-local/storage
PUBLIC_PATH=/var/www/qa-local/public
UPLOAD_PATH=/var/www/qa-local/uploads
LOG_PATH=/var/www/qa-local/logs
SCREENSHOT_PATH=/var/www/qa-local/screenshots

# QA Testing Tools
SELENIUM_HOST=qa-local-selenium
SELENIUM_PORT=4444
BROWSER_NAME=chrome
BROWSER_HEADLESS=false

# Personal Test Accounts
QA_USER_EMAIL=my.qa@octavadigital.cl
QA_USER_PASSWORD=qa_password_123
QA_ADMIN_EMAIL=my.qa.admin@octavadigital.cl
QA_ADMIN_PASSWORD=qa_admin_123

# QA Testing Configuration
TESTING_PARALLEL=true
TESTING_RETRY_TIMES=3
TESTING_TIMEOUT=30
TESTING_MEMORY_LIMIT=1024M

# Personal Test Suites
ENABLED_SUITES="smoke,regression,e2e,api"
SKIP_SUITES="performance,stress"
PRIORITY_SUITES="critical,high"

# QA Reporting
REPORT_PATH=/var/www/qa-local/reports
SCREENSHOT_ON_FAILURE=true
VIDEO_RECORDING=true
CONSOLE_LOGS=true

# Personal API Testing
API_TEST_TOKEN=qa_local_token_123
API_MOCK_RESPONSES=true
API_TEST_TIMEOUT=30
API_RETRY_ATTEMPTS=3

# QA Performance Testing
PERFORMANCE_TESTING=true
PERFORMANCE_THRESHOLD=1000
LOAD_TESTING=false
STRESS_TESTING=false

# Personal Monitoring
ERROR_MONITORING=true
PERFORMANCE_MONITORING=true
NETWORK_MONITORING=true
DATABASE_MONITORING=true

# QA Mail Testing
MAIL_CATCH_ALL=true
MAIL_FORWARD_TO=my.qa@octavadigital.cl
MAIL_SAVE_PATH=/var/www/qa-local/emails

# QA Payment Testing (Sandbox)
STRIPE_KEY=pk_test_qa_local_123
STRIPE_SECRET=sk_test_qa_local_456
PAYPAL_MODE=sandbox
PAYPAL_SANDBOX_EMAIL=my.qa@octavadigital.cl

# Personal Debug Settings
DEBUG_QUERIES=true
DEBUG_EVENTS=true
DEBUG_CACHE=true
DEBUG_MAIL=true
DEBUG_QUEUE=true

# QA Feature Testing
FEATURE_FLAGS_ENABLED=true
EXPERIMENTAL_FEATURES=true
BETA_FEATURES=true
TEST_NEW_FEATURES=true

# Personal Test Data
SEED_TEST_DATA=true
GENERATE_FAKE_DATA=true
PRESERVE_TEST_DATA=false
CLEANUP_INTERVAL=hourly

# QA Browser Testing
CHROME_BINARY=/usr/bin/google-chrome
FIREFOX_BINARY=/usr/bin/firefox
SAFARI_BINARY=/Applications/Safari.app/Contents/MacOS/Safari
BROWSER_TIMEOUT=30000

# Personal Test Output
TEST_OUTPUT_FORMAT=html,junit,json
TEST_REPORT_THEME=dark
SAVE_CONSOLE_OUTPUT=true
SAVE_NETWORK_LOGS=true

# QA Notifications
NOTIFY_ON_FAILURE=true
NOTIFY_ON_SUCCESS=false
NOTIFICATION_EMAIL=my.qa@octavadigital.cl
SLACK_WEBHOOK=https://hooks.slack.com/services/my-qa-hook

# Personal Tools Integration
JIRA_PROJECT=QA-LOCAL
JIRA_USERNAME=my.qa.user
JIRA_API_TOKEN=qa_local_jira_token
CONFLUENCE_SPACE=QA-LOCAL

# QA Environment Access
QA_ACCESS_TOKEN=qa_local_access_123
QA_IP_WHITELIST=office,home
QA_VPN_REQUIRED=false
QA_SSH_KEY=~/.ssh/qa_local_key

# Personal Testing Schedule
TEST_SCHEDULE="0 */4 * * *"
RETRY_FAILED=true
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY=300

# QA Security Testing
SECURITY_SCAN_ENABLED=true
VULNERABILITY_SCAN=true
PENETRATION_TESTING=false
SECURITY_REPORT_PATH=/var/www/qa-local/security

# Personal Preferences
PREFERRED_BROWSER=chrome
PREFERRED_EDITOR=vscode
COLOR_SCHEME=dark
AUTO_OPEN_REPORTS=true

# QA Environment Notice
QA_NOTICE=true
QA_NOTICE_TEXT="üîç Personal QA Testing Environment"
QA_ENVIRONMENT_BANNER="Local QA Testing"

# WARNING:
# - Personal QA testing configuration
