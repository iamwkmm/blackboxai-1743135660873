# Sandbox Application
APP_NAME=OctavaDigital-Sandbox
APP_ENV=sandbox
APP_KEY=base64:sandbox123456789sandbox123456789san
APP_DEBUG=true
APP_URL=https://sandbox.octavadigital.cl
APP_TIMEZONE=America/Santiago
APP_LOCALE=es
APP_CURRENCY=CLP

# Sandbox Database (Isolated)
DB_CONNECTION=mysql
DB_HOST=sandbox-db.octavadigital.cl
DB_PORT=3306
DB_DATABASE=sandbox_octavadigital
DB_USERNAME=sandbox_user
DB_PASSWORD=sandbox_password
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci

# Sandbox Redis
REDIS_HOST=sandbox-redis.octavadigital.cl
REDIS_PASSWORD=sandbox_redis_password
REDIS_PORT=6379
REDIS_DB=0

# Sandbox Mail (Catch-all)
MAIL_MAILER=array
MAIL_FROM_ADDRESS=sandbox@octavadigital.cl
MAIL_FROM_NAME="${APP_NAME} Sandbox"

# Sandbox Storage
AWS_ACCESS_KEY_ID=sandbox_aws_key
AWS_SECRET_ACCESS_KEY=sandbox_aws_secret
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=sandbox-octavadigital
AWS_URL=https://sandbox-cdn.octavadigital.cl
AWS_ENDPOINT=https://s3.us-east-1.amazonaws.com
AWS_USE_PATH_STYLE_ENDPOINT=false

# Sandbox Environment
SANDBOX_ENABLED=true
SANDBOX_MODE=unrestricted
SANDBOX_RESET_INTERVAL=86400
SANDBOX_AUTO_CLEANUP=true
SANDBOX_ISOLATION=true

# Sandbox Access
SANDBOX_ACCESS_TOKEN=required
SANDBOX_MAX_USERS=100
SANDBOX_EXPIRES_AT="+30 days"
SANDBOX_IP_WHITELIST="office,vpn"

# Sandbox Features
ENABLE_ALL_FEATURES=true
ENABLE_EXPERIMENTAL=true
ENABLE_DANGEROUS_ACTIONS=true
ENABLE_TEST_ROUTES=true

# Sandbox Testing
SANDBOX_TEST_MODE=true
SANDBOX_TEST_DATA=true
SANDBOX_MOCK_SERVICES=true
SANDBOX_FAKE_DATA=true

# Sandbox Security (Relaxed)
SESSION_DRIVER=file
SESSION_LIFETIME=1440
SESSION_SECURE_COOKIE=false
SANCTUM_STATEFUL_DOMAINS=sandbox.octavadigital.cl,localhost

# Sandbox Payment Gateways (Test Mode)
STRIPE_KEY=pk_test_sandbox
STRIPE_SECRET=sk_test_sandbox
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=sandbox_paypal_id
PAYPAL_CLIENT_SECRET=sandbox_paypal_secret

# Sandbox Paths
STORAGE_PATH=/var/www/sandbox/storage
PUBLIC_PATH=/var/www/sandbox/public
UPLOAD_PATH=/var/www/sandbox/uploads
LOG_PATH=/var/www/sandbox/logs

# Sandbox Notice
SANDBOX_NOTICE=true
SANDBOX_NOTICE_TEXT="üèñÔ∏è Sandbox Environment - Safe for Experimentation"
SANDBOX_BANNER_TEXT="Sandbox Mode - Test Freely"
SANDBOX_WATERMARK=true

# Sandbox Data
SANDBOX_SEED_DATA=true
SANDBOX_SAMPLE_DATA=true
SANDBOX_TEST_ACCOUNTS=true
SANDBOX_DEMO_CONTENT=true

# Sandbox Monitoring
SANDBOX_MONITORING=true
SANDBOX_LOGGING=verbose
SANDBOX_PROFILING=true
SANDBOX_DEBUGGING=true

# Sandbox Limits
SANDBOX_RATE_LIMIT=1000,1
SANDBOX_STORAGE_LIMIT=10240
SANDBOX_MEMORY_LIMIT=512M
SANDBOX_TIMEOUT=300

# Sandbox Tools
SANDBOX_CONSOLE_ENABLED=true
SANDBOX_API_EXPLORER=true
SANDBOX_DEBUG_TOOLBAR=true
SANDBOX_QUERY_ANALYZER=true

# Sandbox Testing Tools
SANDBOX_UNIT_TESTING=true
SANDBOX_INTEGRATION_TESTING=true
SANDBOX_PERFORMANCE_TESTING=true
SANDBOX_LOAD_TESTING=true

# Sandbox API
API_DEBUG=true
API_STRICT=false
API_THROTTLE=0,1
API_DOCUMENTATION=true

# Sandbox Development
DEBUG_LEVEL=verbose
LOG_LEVEL=debug
QUERY_LOG=true
ERROR_REPORTING=-1

# Sandbox Reset
SANDBOX_RESET_ENABLED=true
SANDBOX_RESET_SCHEDULE="0 0 * * *"
SANDBOX_BACKUP_BEFORE_RESET=true
SANDBOX_PRESERVE_UPLOADS=false

# Sandbox Accounts
SANDBOX_ADMIN_EMAIL=sandbox.admin@octavadigital.cl
SANDBOX_ADMIN_PASSWORD=sandbox123
SANDBOX_TEST_USER_EMAIL=sandbox.user@octavadigital.cl
SANDBOX_TEST_USER_PASSWORD=sandbox123

# Sandbox Services
SANDBOX_SERVICES="email,sms,payment,storage"
SANDBOX_EXTERNAL_APIS="maps,analytics,social"
SANDBOX_THIRD_PARTY="all"
SANDBOX_INTEGRATIONS="all"

# Sandbox Support
SANDBOX_SUPPORT_EMAIL=sandbox.support@octavadigital.cl
SANDBOX_DOCUMENTATION=https://sandbox.octavadigital.cl/docs
SANDBOX_HELP_CENTER=https://sandbox.octavadigital.cl/help
SANDBOX_FAQ=https://sandbox.octavadigital.cl/faq

# Sandbox Cleanup
CLEANUP_ENABLED=true
CLEANUP_INTERVAL=86400
CLEANUP_OLD_DATA=true
CLEANUP_THRESHOLD=30

# Sandbox Performance
OPTIMIZE_IMAGES=false
MINIFY_HTML=false
COMPRESS_OUTPUT=false
CACHE_ENABLED=false

# Sandbox Notifications
NOTIFICATION_ENABLED=true
NOTIFICATION_CHANNEL="email,slack"
NOTIFICATION_EVENTS="error,warning,info"
NOTIFICATION_RECIPIENTS="sandbox.team@octavadigital.cl"