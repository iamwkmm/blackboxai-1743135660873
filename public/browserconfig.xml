<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
    <msapplication>
        <tile>
            <square70x70logo src="/images/tiles/small.png"/>
            <square150x150logo src="/images/tiles/medium.png"/>
            <wide310x150logo src="/images/tiles/wide.png"/>
            <square310x310logo src="/images/tiles/large.png"/>
            <TileColor>#3b82f6</TileColor>
        </tile>
        <notification>
            <polling-uri src="https://www.octavadigital.cl/api/notifications/win-tile"/>
            <polling-uri2 src="https://backup.octavadigital.cl/api/notifications/win-tile"/>
            <frequency>30</frequency>
            <cycle>1</cycle>
        </notification>
        <badge>
            <polling-uri src="https://www.octavadigital.cl/api/notifications/win-badge"/>
            <polling-uri2 src="https://backup.octavadigital.cl/api/notifications/win-badge"/>
            <frequency>30</frequency>
        </badge>
    </msapplication>
</browserconfig>